{
  "name": "node-api",
  "version": "1.0.0",
  "description": "Node.js API приложение",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.local.ts --exec ts-node",
    "dev:ts": "ts-node-dev --respawn --transpile-only src/server.local.ts",
    "build:api": "tsc api/index.ts --outDir dist/api --module commonjs --target es2020",
    "vercel-build": "npm run build",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": ["api", "nodejs", "express", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.0",
    "vercel": "^50.1.3",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.8.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "^24.x"
  }
}
