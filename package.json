{
  "name": "node-api",
  "version": "1.0.0",
  "description": "Node.js API приложение",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:ts": "ts-node-dev src/server.ts",
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write src",
    "prestart": "npm run build"
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.8.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
